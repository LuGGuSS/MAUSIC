<?xml version="1.0" encoding="utf-8"?>

<toolkit:Popup xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:models="clr-namespace:MAUSIC.Models"
             xmlns:views="clr-namespace:MAUSIC.Views"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             x:DataType="models:MoreModel"
             x:Class="MAUSIC.Views.MoreView">

    <StackLayout
        BindableLayout.ItemsSource="{Binding MoreOptions}">
        <BindableLayout.ItemTemplate>
            <DataTemplate
                x:Key="MoreTemplate"
                x:DataType="models:MoreOptionModel">
                <views:MoreOption/>
            </DataTemplate>

        </BindableLayout.ItemTemplate>

    </StackLayout>

</toolkit:Popup>